{{!-- 
Це код container файлу для summary сторінки, 
в якому міститься код, котрий буде віддаватися по шляху (PATH) - "/summary"
--}}

<div>
    {{#with main}}
    <main>
        <section>
            <span>{{> title data = "My educationS" big=true}}</span>
            <ul>
                {{#each educations}}
                <li>{{name}}
                    {{#unless isEnd includeZero=true}}
                     (ще в процесі) 
                    {{/unless}}
                </li>
                {{/each}}
            </ul>
        </section>
        <section>
            {{> title data="My certificates" big=false}}
            <ul>
                {{#each certificates}}
                <li>{{name}} ({{id}})

                </li>
                {{/each}}
            </ul>
        </section>
    </main>
    {{/with}}
</div>

