<main>
    {{#with heading}}
        {{#with main}}
    <div>
        <h1 class="text-capitalize">{{title}}</h1>
        <p>{{text}}</p>
        {{#with button_text}}
        <button class="btn btn-primary">
            <span>{{solid}}</span>
        </button>
        
        <br>
        <br>
        <button class="btn btn-outline-primary">{{outline}}</button>
     </div>
        {{/with}} {{!-- button_text --}}
       {{/with}} {{!-- main --}}
        <br>
        <br>
        {{#with second}}
    <div>
            <h2>{{title}}</h2>
        <p>{{text}}</p>
        {{#with button_text}}
        <button class="btn btn-secondary">
            <span class="text-capitalize">{{solid}}</span>
        </button>
        <br>
        <br>
        <button class="btn btn-outline-secondary">{{outline}}</button>
    </div>
        {{/with}} {{!-- button_text --}}
       {{/with}} {{!-- second --}}
        <br>
        <br>
            {{#with subblock}}
        <div>
            <h3>{{title}}</h3>
            <p>{{text}}</p>
                {{#with button_text}}
                <button class="btn btn-dark">
                    <span class="text-capitalize">{{solid}}</span>
                </button>
                <br>
                <br>
                <button class="btn btn-outline-dark">{{outline}}</button>
            </div>
                {{/with}} {{!-- button_text --}}
            {{/with}} {{!-- subblock --}}                               
        <br>
        <br>
        {{/with}} {{!-- heading --}}

        {{#with nested_display}}
            <div>
                     <div class="display-1">{{title}}</div>
                {{#with children_display}}
                    <div class="display-4 text-center">{{title}}n</div>
                    <p>{{text}}</p>
                        <button class="btn btn-outline-warning">
                            <span>{{button.text}}</span>
                        </button>
                {{/with}} {{!-- children_display --}}
            </div>
       
            <br>
            <br>
            <div>
                <div class="display-6 text-capitalize">{{header_text}}</div>
                <br>
                {{#with button}}
                 <button class="btn btn-info">
                    <span>{{text}}</span>
                </button>
                {{/with}} {{!-- button --}}
            </div>
        {{/with}} {{!-- nested_display --}}
            <br>
            <br>
        {{#with paragraph}}
                <div>
                    <p class="text-uppercase">{{title}}</p>
                    <br>
                    <p class="text-lowercase">{{text}}</p>
                    <br>
                    {{#with button}}
                        <a href="{{url}}" class="text-capitalize">
                            {{text}}
                        </a>
                    {{/with}} {{!-- button --}}
                </div>
              
                <br>
                <br>
                {{#each list_name}}
                <div>
                    <ul>
                        <li><a href="{{url}}" class="text-capitalize">{{text}}</a></li>
                    </ul>
                </div>
               {{/each}} {{!-- list_name --}}
        {{/with}} {{!-- paragraph --}}
  

        <br>
        <br>
       
    
 
        <br>
        <br>
    <div class="card">
        <div class="card-body">This is content</div>
    </div>
 
</main>