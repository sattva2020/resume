<main>
    <div class="card">
        <div class="card-body">
            {{#with page_header}}
                <div>
                    <h1 class="text-center">{{title}}</h1>
                    <p class="text-center text-uppercase">{{description}}</p>
                </div>
            {{/with}} {{!-- page_header --}}
        </div>
    </div>
   
    <section>
        {{#with section}}

            <div class="card">
                <div class="card-body">
                {{#with about_us}}

                    {{#with title_list}}
                    <div class="display-6 text-end text-uppercase">{{main}}</div>
                    <br>
                    <h2 class="text-end ">{{sub}}</h2>

                    {{/with}} {{!-- title_list --}}
                    <br>
                    <br>       
                    <div>
                        <button class="btn btn-outline-info">{{button.text}}</button>
                    </div>
                {{/with}} {{!-- about_us --}}
                </div>
            </div>

        <br>
        <br>

        <div class="card">
            
                {{#with advantage}}

                        <div class="display-5 text-capitalize">{{title}}</div>
                        <h2 class="text-end ">{{sub}}</h2>
                        <br>
                            <div class="card-body">

                                {{#with efficiency}}
                                    <div class="display-4 text-capitalize">{{title}}</div>
                                    <br>
                                    <h6>{{sub_title}}</h6>
                                    <p>{{description}}</p>
                                <div>
                                    <button class="btn btn-outline-warning">{{button}}</button>
                                </div>

                                {{/with}} {{!-- efficiency --}}
                            </div>
                        </div>
                        <br>
                        <br>
                    <div class="card">
                        <div class="card-body">
                            {{#with innovation}}
                                <div class="display-4 text-capitalize text-center">{{title}}</div>
                                <br>
                                <h6 class="text-center">{{sub_title}}</h6>
                                <p class="text-center">{{description}}</p>
                                <div>
                                    <button class="btn btn-outline-warning">{{button}}</button>
                                </div>
                            {{/with}} {{!-- innovation --}}
                        </div>
                    </div>
                    <br>
                    <br>
                    <div class="card">
                        <div class="card-body">
                            {{#with collaboration}}
                            <div class="display-4 text-capitalize text-end">{{title}}</div>
                            <br>
                            <h6 class="text-end">{{sub_title}}</h6>
                            <p class="text-end">{{description}}</p>
                            <div>
                                <button class="btn btn-outline-warning">{{button}}</button>
                            </div>
                            {{/with}} {{!-- innovation --}}
                        </div>
                    </div>

                {{/with}} {{!-- advantage --}}
        <br>
        <br>
        <div class="card">
            <div class="card-body">
        {{#with agreement}}
                <div class="display-5 text-capitalize">{{title}}</div>
                {{#with info}}
                <br>
                <h5>{{sub_title}}</h5>
                    {{#with detail}}
                    <br>
                    <h5>{{text}}</h5>
                    <br>
                    <div>
                        <button class="btn btn-success">{{button}}</button>
                    </div>
                    {{/with}} {{!-- detail --}}

                {{/with}} {{!-- info --}}

            {{/with}} {{!-- agreement --}}
            </div>
        </div>
        <br>
        {{/with}} {{!-- section --}}

    </section>
</main>